      "name": "Launch Edge",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:5173/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "esno",
      "type": "node",
      "request": "launch",
      "args": [
        "./node_modules/esno/esno.js",
        "${file}"
      ],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "启动程序",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${file}"
    },
    {
      "type": "msedge",
      "name": "http://127.0.0.1:5502/%E8%AF%BB%E4%B9%A6%E7%B3%BB%E5%88%97/build-your-won-react/index.html",
