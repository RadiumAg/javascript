{
    "presets": [
      [
          // 为目标浏览器中没有的功能加载转换插件。
        "@babel/env",
        {
          "targets": {
            "edge": "17",
            "firefox": "60",
            "chrome": "67",
            "safari": "11.1"
          },
          "useBuiltIns": "usage",
          "corejs": "3.6.5"
        }
      ]
    ],
    "plugins": ["@babel/plugin-transform-arrow-functions"]
  }